<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="100dp"
    android:background="#ffffff">

    <FrameLayout
        android:layout_width="match_parent"
        android:background="?attr/selectableItemBackground"
        android:layout_height="match_parent"
        android:id="@+id/container">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Training 1"
            android:id="@+id/textView9"
            android:layout_gravity="left|top"
            android:textColor="@color/grey_text_color"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="56dp">

            <binaryblitz.athleteapp.Custom.BoldRobotoTextView
                android:layout_width="wrap_content"
                android:layout_height="28dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="5 / 11"
                android:id="@+id/textView5fdfd"
                android:layout_gravity="left"
                android:layout_marginLeft="16dp"
                android:textColor="@color/grey_text_color"
                android:textStyle="bold"
                android:background="@drawable/date_text_border_back"
                android:gravity="center"
                android:paddingLeft="12dp"
                android:paddingRight="12dp"
                android:textSize="14sp"
                android:textIsSelectable="false" />

            <binaryblitz.athleteapp.Custom.BoldRobotoTextView
                android:layout_width="wrap_content"
                android:layout_height="28dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="80 MIN"
                android:id="@+id/textView5"
                android:layout_gravity="left"
                android:layout_marginLeft="16dp"
                android:textColor="@color/grey_text_color"
                android:textStyle="bold"
                android:background="@drawable/date_text_border_back"
                android:gravity="center"
                android:textSize="14sp"
                android:textIsSelectable="false"
                android:paddingLeft="12dp"
                android:paddingRight="12dp" />

            <binaryblitz.athleteapp.Custom.BoldRobotoTextView
                android:layout_width="wrap_content"
                android:layout_height="28dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="5 / 11"
                android:id="@+id/textView5fdfddsds"
                android:layout_gravity="left"
                android:layout_marginLeft="16dp"
                android:textColor="@color/grey_text_color"
                android:textStyle="bold"
                android:background="@drawable/date_text_border_back"
                android:gravity="center"
                android:paddingLeft="12dp"
                android:paddingRight="12dp"
                android:textSize="14sp"
                android:textIsSelectable="false" />

            <binaryblitz.athleteapp.Custom.BoldRobotoTextView
                android:layout_width="wrap_content"
                android:layout_height="28dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="80 MIN"
                android:id="@+id/textView5fdf"
                android:layout_gravity="left"
                android:layout_marginLeft="16dp"
                android:textColor="@color/grey_text_color"
                android:textStyle="bold"
                android:background="@drawable/date_text_border_back"
                android:gravity="center"
                android:textSize="14sp"
                android:textIsSelectable="false"
                android:paddingLeft="12dp"
                android:paddingRight="12dp" />

        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_gravity="center_horizontal|bottom"
            android:background="#b6b6b6"></FrameLayout>

        <FrameLayout
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:background="@drawable/thin_green_circle"
            android:layout_gravity="bottom|right"
            android:layout_marginRight="-35dp"
            android:layout_marginBottom="-40dp"
            android:layout_marginTop="40dp">

            <ImageView
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:id="@+id/imageView9"
                android:layout_gravity="center"
                android:src="@drawable/done_green"
                android:layout_marginBottom="16dp"
                android:layout_marginRight="12dp" />
        </FrameLayout>
    </FrameLayout>

</FrameLayout>